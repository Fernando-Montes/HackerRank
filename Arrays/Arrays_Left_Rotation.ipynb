    "# Arrays: Left Rotation\n",
    "def rotLeft(d, a):\n",
    "    dnew = [0 for i in range(len(d))]\n",
    "    j=0\n",
    "    for i in range(a, len(d)):\n",
    "        dnew[j] = d[i]\n",
    "        j = j+1\n",
    "    for i in range(0, a):\n",
    "        dnew[j] = d[i]\n",
    "        j = j+1\n",
    "    return(dnew)"
   ]
  },
